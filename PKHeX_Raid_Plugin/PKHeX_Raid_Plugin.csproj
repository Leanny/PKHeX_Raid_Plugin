  <ItemGroup>
    <Reference Include="PKHeX.Drawing">
      <HintPath>..\..\..\PKHeX\dev\PKHeX.Drawing.dll</HintPath>
    </Reference>
    <Reference Include="PKHeX.Drawing.Misc">
      <HintPath>..\..\..\PKHeX\dev\PKHeX.Drawing.Misc.dll</HintPath>
