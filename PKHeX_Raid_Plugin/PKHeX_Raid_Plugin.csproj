    <Reference Include="PKHeX.Drawing.PokeSprite">
      <HintPath>..\..\..\PKHeX\dev\PKHeX.Drawing.PokeSprite.dll</HintPath>
    </Reference>
    <Reference Include="System.Windows.Forms" />
